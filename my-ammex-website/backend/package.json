{
    "name": "ammex-website-server",
    "version": "1.0.0",
    "description": "Backend server for Ammex Website",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "setup-db": "node setup-database.js",
      "db:sync": "node -e \"require('./config/db').connectDB().then(() => require('./models-postgres').sequelize.sync({force: false, alter: true})).then(() => process.exit(0))\""
    },
    "dependencies": {
      "express": "4.18.2",
      "pg": "^8.11.3",
      "pg-hstore": "^2.3.4",
      "sequelize": "^6.35.2",
      "dotenv": "16.3.1",
      "cors": "2.8.5",
      "jsonwebtoken": "9.0.2",
      "bcryptjs": "2.4.3",
      "express-validator": "7.0.1",
      "morgan": "1.10.0",
      "body-parser": "1.20.2",
      "cookie": "0.7.0",
      "path-to-regexp": "6.2.1",
      "send": "0.18.0",
      "@tensorflow/tfjs-node": "^4.17.0",
      "simple-statistics": "^7.8.3",
      "moment": "^2.29.4",
      "lodash": "^4.17.21"
    },
    "devDependencies": {
      "nodemon": "3.0.1",
      "sequelize-cli": "^6.6.2"
    }
  } 